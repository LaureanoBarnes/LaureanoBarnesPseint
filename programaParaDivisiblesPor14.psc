Algoritmo programaParaDivisiblesPor14
	Definir num Como Real;
	escribir "dame un numero para dividir";
	Leer num;
	si num mod 14=0 Entonces
		Escribir "es divisible por 14";
	FinSi
	si num mod 14 <> 0 Entonces
		Escribir "no es divisible";
	FinSi
FinAlgoritmo
